<resources>
    <attr name="keyTypeEnum" format="enum">
        <enum name="KEYTYPE_CHARACTER" value="0" />
        <enum name="KEYTYPE_CONTROL" value="1" />
        <enum name="KEYTYPE_MODIFIER" value="2" />
        <enum name="KEYTYPE_FUNCTION" value="3" />
    </attr>
    <attr name="keyCodeString" format="string" />
    <attr name="keySymbolString" format="string" />
    <attr name="keyShiftSymbolString" format="string" />
    <declare-styleable name="KeyImageButton">
        <attr name="keyTypeEnum" />
        <attr name="keyCodeString" />
        <attr name="keySymbolString" />
        <attr name="keyShiftSymbolString" />
    </declare-styleable>
    <declare-styleable name="KeyButton">
        <attr name="keyTypeEnum" />
        <attr name="keyCodeString" />
        <attr name="keySymbolString" />
        <attr name="keyShiftSymbolString" />
    </declare-styleable>
</resources>